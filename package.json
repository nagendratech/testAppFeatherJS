  "scripts": {
    "start": "docker-compose up",
    "build-dev": "docker-compose build --no-cache",
    "qa": "docker-compose -f qa.yml up",
    "build-qa": "docker-compose -f qa.yml build --no-cache",
    "watch": "gulp && webpack -w",
    "serve": "nodemon --watch server --ignore 'server/compiled-ssr.js' --watch config server/",
    "test": "mocha test/ --recursive",
    "dev": "npm-run-all --parallel watch serve",
    "build": "gulp && webpack",
    "production": "pm2 start server/ --no-daemon"
